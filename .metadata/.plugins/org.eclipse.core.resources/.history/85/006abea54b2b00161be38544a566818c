package br.com.brq.financialhealth.util;

public class CalculoPorIdade {
	
	
	class GuardaValores{
		int idade;
		double investimento;
		double despesaFixa;
		double despesaVariavel;
		
	}
		
	void calculaTotal(int idade){

		GuardaValores gv = new GuardaValores();
		
		if(idade >65){
			
			gv.investimento = 0.1;
			
		}
		
		}
		
	}

